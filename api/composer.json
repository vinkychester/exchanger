{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-imap": "*",
        "ext-json": "*",
        "ext-redis": "*",
        "api-platform/core": "^2.6",
        "crpt/rate": "*",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^1.6 || ^2.0",
        "doctrine/mongodb-odm-bundle": "^4.2",
        "doctrine/orm": "^2.4.5",
        "gesdinet/jwt-refresh-token-bundle": "^0.11.1",
        "google/apiclient": "^2.8",
        "hackzilla/password-generator": "^1.5",
        "imagine/imagine": "^1.2",
        "itlab-studio/api-client": "^2.0",
        "itlab-studio/calculation": "*",
        "lexik/jwt-authentication-bundle": "^2.8",
        "nelmio/cors-bundle": "^1.5 || ^2.0",
        "php-imap/php-imap": "^4.1",
        "phpdocumentor/reflection-docblock": "^3.0 || ^4.0 || ^5.0",
        "phpoffice/phpspreadsheet": "^1.11",
        "ramsey/uuid-doctrine": "^1.6",
        "sonata-project/google-authenticator": "^2.2",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/error-handler": "5.1.*",
        "symfony/expression-language": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.1.*",
        "symfony/mailer": "5.1.*",
        "symfony/mercure-bundle": "^0.2.5",
        "symfony/messenger": "5.1.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/notifier": "5.1.*",
        "symfony/orm-pack": "^1.1",
        "symfony/property-info": "5.2.2",
        "symfony/security-bundle": "5.1.*",
        "symfony/telegram-notifier": "5.1.*",
        "symfony/twig-bundle": "5.1.*",
        "symfony/validator": "5.1.*",
        "symfony/yaml": "5.1.*",
        "twig/cssinliner-extra": "^3.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/inky-extra": "^3.0",
        "twig/twig": "^2.12|^3.0",
        "web-token/jwt-framework": "^2.1",
        "web-token/jwt-signature": "^2.1",
        "webonyx/graphql-php": "^14.3",
        "ext-gd": "*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "fzaninotto/faker": "^1.9",
        "symfony/maker-bundle": "^1.19",
        "symfony/stopwatch": "^5.1",
        "symfony/web-profiler-bundle": "^5.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    },
    "repositories": {
        "private-packagist": {
            "type": "composer",
            "url": "https://repo.packagist.com/it-lab-studio/"
        },
        "packagist.org": false
    }
}

